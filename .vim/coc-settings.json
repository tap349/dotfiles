{
  "coc.preferences.formatOnSaveFiletypes": [
    "elixir",
    "go",
    "javascript",
    "javascriptreact",
    "json",
    "typescript",
    "typescriptreact",
    "yaml"
  ],
  "diagnostic.errorSign": "âœ—",
  "diagnostic.maxWindowHeight": 16,
  "list.normalMappings": {
    "v": "action:vsplit"
  },
  "elixirLS.dialyzerEnabled": false,
  "go": {
    "goplsArgs": ["-remote=auto", "-logfile", "/tmp/gopls.log"],
    "goplsPath": "~/.asdf/shims/gopls",
